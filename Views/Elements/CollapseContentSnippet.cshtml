@using Orchard.Layouts.Helpers

@{
    Style.Require("CollapseContent").AtHead();

    var root = Html.SnippetField("Root");
    var start = Html.SnippetField("Start");
    var end = Html.SnippetField("End");

    using (Script.Foot())
    {
        Script.Require("CollapseContent");

        <script>
        collapseContent("@root", @start, @end);
        </script>
    }
}