@* This shape template is overriding ~/Orchard.Core/Shapes/Views/MenuItemLink.cshtml *@

@if (Model.IsDropdown)
{
    <a class="dropdown-toggle" href="#" role="button" data-toggle="dropdown" aria-expanded="false">
        @Model.Text <span class="caret"></span>
    </a>
}
else
{
    <a href="@Model.Href">@Model.Text</a>
}
